{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "start:dev",
        "program": "${workspaceFolder}/node_modules/@nestjs/cli/bin/nest.js",
        "args": ["start", "--watch"],
        "runtimeArgs": ["--nolazy"],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "openOnSessionStart",
        "sourceMaps": true,
        "env": {
          "APP_PORT": "3000",
          "GRPC_HOST": "localhost",
          "PORT": "5000"
        },
        "skipFiles": [
          "${workspaceFolder}/node_modules/**/*.js",
          "<node_internals>/**/*.js"
        ],
        "outputCapture": "std"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "start:debug",
        "program": "${workspaceFolder}/node_modules/@nestjs/cli/bin/nest.js",
        "args": ["start", "--watch", "--inspect-brk=9229"],
        "runtimeArgs": ["--nolazy"],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "openOnSessionStart",
        "sourceMaps": true,
        "env": {
          "APP_PORT": "3000",
          "GRPC_HOST": "localhost",
          "PORT": "5000"
        },
        "skipFiles": [
          "${workspaceFolder}/node_modules/**/*.js",
          "<node_internals>/**/*.js"
        ],
        "outputCapture": "std"
      },
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to Running NestJS",
        "port": 9229,
        "restart": true,
        "protocol": "inspector",
        "timeout": 30000,
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }
  